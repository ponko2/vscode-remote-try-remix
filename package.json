  "packageManager": "pnpm@10.17.1",
  "private": true,
  "scripts": {
    "build": "react-router build",
    "build-storybook": "storybook build",
    "dev": "react-router dev",
    "fix": "run-s fix:eslint fix:prettier fix:prisma-case fix:prisma",
    "fix:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "fix:prettier": "prettier --write .",
    "fix:prisma": "prisma format",
    "fix:prisma-case": "prisma-case-format --file prisma/schema.prisma",
    "generate": "run-p --aggregate-output -c -l generate:*",
    "generate:prisma": "prisma generate",
    "generate:react-router": "react-router typegen",
    "lint": "run-p --aggregate-output -c -l lint:*",
    "lint:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:prettier": "prettier --check .",
    "prepare": "node .husky/install.mjs",
    "setup": "corepack enable pnpm",
    "start": "react-router-serve ./build/server/index.js",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --passWithNoTests",
    "test-storybook": "vitest --project=storybook",
    "typecheck": "tsc --build --noEmit"
  },
  "type": "module"
}
