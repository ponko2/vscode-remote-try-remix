    "dev": "remix vite:dev",
    "fix": "run-s fix:eslint fix:prettier fix:prisma-case fix:prisma",
    "fix:eslint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint --fix .",
